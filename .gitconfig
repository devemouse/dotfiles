[user]
   name = Dariusz Synowiec
   email = darek129@gmail.com
[color]
   #status = always
   #branch = always
   #diff = always
   #interactive = always
   ui = auto
[color "branch"]
   current = yellow reverse
   local = yellow
   remote = green
[color "diff"]
   meta = yellow bold
   frag = magenta bold
   old = red bold
   new = green bold
[color "status"]
   added = yellow
   changed = green
   untracked = cyan
[core]
   ignoreCygwinFSTricks  = true
   autocrlf = input
   safecrlf = false
   pager = "less"
[alias]
   cim  = "commit -m"
   cima = "commit -a -m"
   co   = "checkout"
   st   = "status"
   br   = "branch"
   di   = "diff --no-ext-diff"
   die  = "diff"
[branch]
   autosetupmerge = true
[diff]
    external = git_diff_wrapper.sh
   
